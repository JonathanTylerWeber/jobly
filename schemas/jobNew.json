{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "title",
        "company_handle"
    ],
    "properties": {
        "title": {
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": [
                "Chief of Staff"
            ]
        },
        "salary": {
            "type": "integer",
            "default": 0,
            "title": "The salary Schema",
            "examples": [
                110000
            ]
        },
        "equity": {
            "type": "number",
            "default": 0.0,
            "title": "The equity Schema",
            "examples": [
                0.016
            ]
        },
        "company_handle": {
            "type": "string",
            "default": "",
            "title": "The company_handle Schema",
            "examples": [
                "scott-smith"
            ]
        }
    },
    "examples": [
        {
            "title": "Chief of Staff",
            "salary": 110000,
            "equity": 0.016,
            "company_handle": "scott-smith"
        }
    ]
}